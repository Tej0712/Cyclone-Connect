<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homepagemain1">


    <Button
        android:id="@+id/Help"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="412dp"
        android:text="Help"
        android:visibility="visible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Weather"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Notes"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="284dp"
        android:layout_marginRight="20dp"
        android:text="Notes"
        android:visibility="visible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Weather"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Weather"
        android:layout_width="150dp"
        android:layout_height="66dp"
        android:layout_marginStart="18dp"
        android:text="Weather"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Maps"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.616"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/ChatwithAdvisor"
        android:layout_width="150dp"
        android:layout_height="66dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="149dp"
        android:layout_marginBottom="76dp"
        android:text="Advisor Chat"
        app:layout_constraintBottom_toTopOf="@+id/courses_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.267"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Weather"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Calendar"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="284dp"
        android:text="Calendar"
        app:layout_constraintEnd_toStartOf="@+id/Notes"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <Button
        android:id="@+id/profile_button"
        android:layout_width="95dp"
        android:layout_height="65dp"
        android:text="Profile"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:visibility="visible" />

    <Button
        android:id="@+id/courses_button"
        android:layout_width="97dp"
        android:layout_height="64dp"
        android:layout_marginLeft="180dp"
        android:layout_marginEnd="164dp"
        android:text="Head Start"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/Home"
        android:layout_width="51dp"
        android:layout_height="52dp"
        android:layout_marginStart="25dp"


        android:background="@drawable/homebutton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/courses_button"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.988"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Maps"
        android:layout_width="150dp"
        android:layout_height="64dp"
        android:text="Maps"
        app:layout_constraintBottom_toTopOf="@+id/profile_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.645"
        app:layout_constraintStart_toEndOf="@+id/ChatwithAdvisor"
        app:layout_constraintTop_toBottomOf="@+id/Help"
        app:layout_constraintVertical_bias="0.396" />

    <Button
        android:id="@+id/CycloneAI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cyclone AI"
        app:layout_constraintBottom_toBottomOf="@+id/bottomAppBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.881" />


</androidx.constraintlayout.widget.ConstraintLayout>